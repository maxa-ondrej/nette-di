{
    "name": "maxa-ondrej/nette-di",
    "description": "Nette Framework better DI",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Maxa\\Ondrej\\Nette\\DI\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "contributte/di": "^0.5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.1"
        }
    },
    "extra": {
        "enable-patching": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}